<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="tataMotors">
    <parameter name="URL" value="https://www.tatamotorCars.com/"></parameter>

    <test name="ServicingTeam">
        <parameter name="UserName" value="ServicingTeam@tataMotors"></parameter>
        <parameter name="Password" value="Password@123"></parameter>
        <classes >
            <class name="tataMotors.ServicingTeam"></class>
        </classes>
    </test>

    <test name="SalesTeam">
        <parameter name="UserName" value="SalesTeam@tataMotors"></parameter>
        <parameter name="Password" value="Password@123"></parameter>
        <classes>
            <class name="tataMotors.SalesTeam"></class>
        </classes>
    </test>
</suite>